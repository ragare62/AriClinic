  <data name="sqlInvoiceLine.SelectCommand" xml:space="preserve">
    <value>SELECT        invoice_line.invoice_id, invoice_line.invoice_line_id, invoice_line.description, tax_type.name, invoice_line.amount
FROM            invoice_line INNER JOIN
                         tax_type ON invoice_line.tax_type_id = tax_type.tax_type_id
WHERE        (invoice_line.invoice_id = @InvoiceId)</value>
